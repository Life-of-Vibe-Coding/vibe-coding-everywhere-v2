{
  "name": "terminal-pty",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:mobile": "EXPO_PUBLIC_SERVER_URL=http://localhost:3456 npm run -w mobile-cocoa start",
    "proxy": "node server/proxy.js",
    "cloudflare:tunnel": "node scripts/start-cloudflare-tunnel.mjs",
    "cloudflare:tunnel:server": "CLOUDFLARE_TUNNEL_TARGET=http://localhost:3456 node scripts/start-cloudflare-tunnel.mjs",
    "dev:cloudflare": "node scripts/start-dev-cloudflare.mjs",
    "dev:mobile:cloudflare": "EXPO_PUBLIC_CONNECTION_MODE=cloudflare npm run -w mobile-cocoa start",
    "icons:convert": "node scripts/convert-atom-icons.mjs",
    "smoke:sse": "node scripts/smoke-pi-rpc-sse-multi-client.mjs",
    "smoke:concurrent": "node scripts/smoke-pi-rpc-sse-concurrent.mjs",
    "smoke:session-switch": "node scripts/smoke-pi-rpc-sse-session-switch.mjs",
    "smoke:session-folder": "node scripts/smoke-session-folder.mjs"
  },
  "dependencies": {
    "dockerode": "^4.0.9",
    "dotenv": "^17.3.1",
    "express": "^4.21.2",
    "imagetracerjs": "^1.2.6"
  },
  "devDependencies": {
    "eventsource": "^4.1.0",
    "nodemon": "^3.1.11",
    "patch-package": "^8.0.0"
  }
}
