{
  "name": "mobile-cocoa",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prebuild:icons": "node ../../scripts/fetch-workspace-icons.mjs",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "verify:foundation": "node scripts/verify-foundation.mjs",
    "inventory:components": "node scripts/generate-component-inventory.mjs",
    "capture:ui-primitives-baseline": "node scripts/capture-ui-primitives-baseline.mjs",
    "lint:ui-primitives": "node scripts/enforce-ui-primitives-baseline.mjs",
    "phase2:check": "npm run verify:foundation && npm run lint:ui-primitives",
    "lint:ui-primitives:strict": "node scripts/check-ui-primitives-strict.mjs",
    "phase6:check": "npm run phase2:check && npm run lint:ui-primitives:strict",
    "test:phase7:visual": "jest --runInBand src/components/chat/__tests__/InputPanel.visual.test.tsx",
    "test:phase7:a11y": "jest --runInBand src/components/chat/__tests__/InputPanel.a11y.test.tsx",
    "phase7:check": "npm run phase6:check && npm run test:phase7:visual && npm run test:phase7:a11y && npm run phase7:report",
    "metrics:rollout": "node scripts/generate-rollout-metrics.mjs",
    "phase7:report": "npm run metrics:rollout"
  },
  "dependencies": {
    "@expo/html-elements": "^0.12.5",
    "@gluestack-ui/core": "^3.0.12",
    "@gluestack-ui/utils": "^3.0.15",
    "@legendapp/motion": "^2.4.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "clsx": "^2.1.1",
    "expo": "~54.0.33",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.13",
    "expo-device": "~8.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.32.16",
    "expo-screen-orientation": "~9.0.8",
    "expo-status-bar": "~3.0.9",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "^4.2.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-sse": "^1.2.1",
    "react-native-svg": "^15.12.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.7.0",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "uniwind": "^1.3.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|react-native-svg|react-native-safe-area-context|@gluestack-ui/.*)"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ]
  },
  "private": true
}